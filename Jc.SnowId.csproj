<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>.netstandard2.1</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <LangVersion>10</LangVersion>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>1.0.1</PackageVersion>
        <Title>Jc.SnowId</Title>
        <Authors>JackerKun</Authors>
        <Copyright>JackerKun</Copyright>
        <PackageProjectUrl>http://jackerkun.com</PackageProjectUrl>
        <PackageLicenseUrl>http://jackerkun.com</PackageLicenseUrl>
        <Description>16位 雪花ID生成模块</Description>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
      <DocumentationFile>bin\Debug\.netstandard2.1\Jc.SnowId.xml</DocumentationFile>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
      <DocumentationFile>bin\Release\.netstandard2.1\Jc.SnowId.xml</DocumentationFile>
    </PropertyGroup>

</Project>
